  "tokenColors": [
    { "scope": ["comment", "punctuation.definition.comment"], "settings": { "foreground": "#808080", "fontStyle": "italic" } },
    { "scope": ["comment.block.documentation", "comment.documentation"], "settings": { "foreground": "#629755", "fontStyle": "italic" } },

    { "scope": ["keyword", "storage.type", "storage.modifier", "keyword.operator.new", "keyword.control"], "settings": { "foreground": "#CC7832" } },
    { "scope": ["keyword.operator", "punctuation.separator", "punctuation.accessor"], "settings": { "foreground": "#A9B7C6" } },

    { "scope": ["string", "meta.embedded.assembly", "constant.other.symbol"], "settings": { "foreground": "#6A8759" } },
    { "scope": ["string.regexp"], "settings": { "foreground": "#54B7D9" } },
    { "scope": ["constant.numeric"], "settings": { "foreground": "#6897BB" } },
    { "scope": ["constant.language", "variable.language", "support.constant"], "settings": { "foreground": "#CC7832" } },

    { "scope": ["variable.other.constant"], "settings": { "foreground": "#9876AA" } },
    { "scope": ["variable", "meta.definition.variable"], "settings": { "foreground": "#A9B7C6" } },
    { "scope": ["variable.parameter"], "settings": { "foreground": "#A9B7C6", "fontStyle": "italic" } },
    { "scope": ["variable.property"], "settings": { "foreground": "#A9B7C6" } },

    { "scope": ["entity.name.type", "entity.name.class", "support.class"], "settings": { "foreground": "#A9B7C6" } },
    { "scope": ["entity.name.namespace", "support.other.namespace"], "settings": { "foreground": "#A9B7C6" } },
    { "scope": ["entity.name.function", "meta.function-call", "support.function"], "settings": { "foreground": "#FFC66D" } },
    { "scope": ["meta.method", "entity.name.function.method"], "settings": { "foreground": "#FFC66D" } },

    { "scope": ["entity.name.tag", "support.class.component"], "settings": { "foreground": "#E8BF6A" } },
    { "scope": ["entity.other.attribute-name"], "settings": { "foreground": "#BABABA" } },

    { "scope": ["support.type.property-name", "meta.object-literal.key", "support.type.property-name.json"], "settings": { "foreground": "#A9B7C6" } },

    { "scope": ["punctuation.definition.string", "punctuation.definition.parameters", "punctuation.section.embedded"], "settings": { "foreground": "#A9B7C6AA" } },

    { "scope": ["invalid", "invalid.illegal"], "settings": { "foreground": "#FFFFFF", "background": "#FF6B68" } },
    { "scope": ["markup.heading"], "settings": { "foreground": "#6699CC", "fontStyle": "bold" } },
    { "scope": ["markup.bold"], "settings": { "fontStyle": "bold" } },
    { "scope": ["markup.italic"], "settings": { "fontStyle": "italic" } },
    { "scope": ["markup.inline.raw"], "settings": { "foreground": "#6897BB" } },

    { "scope": ["source.php variable.other"], "settings": { "foreground": "#A9B7C6" } },
    { "scope": ["source.php support.function"], "settings": { "foreground": "#FFC66D" } },
    { "scope": ["source.php keyword"], "settings": { "foreground": "#CC7832" } },
    { "scope": ["source.php storage.type", "source.php storage.modifier"], "settings": { "foreground": "#CC7832" } },
    { "scope": ["source.php entity.name.type.class"], "settings": { "foreground": "#A9B7C6" } },
    { "scope": ["comment.block.documentation.phpdoc"], "settings": { "foreground": "#629755", "fontStyle": "italic" } },
    { "scope": ["source.blade support.function.directive.blade"], "settings": { "foreground": "#CC7832" } },
    { "scope": ["source.blade variable.other.blade"], "settings": { "foreground": "#A9B7C6" } }
  ],

  "semanticTokenColors": {
    "comment": "#808080",
    "string": "#6A8759",
    "number": "#6897BB",
    "regexp": "#54B7D9",
    "keyword": "#CC7832",
    "operator": "#A9B7C6",
    "namespace": "#A9B7C6",
    "type": "#A9B7C6",
    "class": "#A9B7C6",
    "interface": "#A9B7C6",
    "enumMember": "#9876AA",
    "function": "#FFC66D",
    "method": "#FFC66D",
    "property": "#A9B7C6",
    "variable": "#A9B7C6",
    "parameter": { "foreground": "#A9B7C6", "italic": true },
    "decorator": "#BBB529",
    "macro": "#BBB529",
    "boolean": "#CC7832",
    "escapeCharacter": "#6897BB"
  }
}
