{
  "name": "jetvibe",
  "displayName": "JetVibe",
  "description": "Sets up VSCode to Jetbrains style and includes ready-to-use extras.",
  "version": "0.0.30",
  "publisher": "igorhaf",
  "engines": {
    "vscode": "^1.84.0"
  },
  "extensionKind": [
    "ui"
  ],
  "icon": "images/icon.png",
  "categories": [
    "Themes",
    "Keymaps"
  ],
  "keywords": [
    "phpstorm",
    "darcula",
    "jetbrains",
    "keymap",
    "theme",
    "icons"
  ],
  "contributes": {
    "commands": [
      {
        "command": "jetvibe.installFont",
        "title": "JetVibe: Install JetBrains Mono"
      },
      {
        "command": "jetvibe.installNerdFont",
        "title": "JetVibe: Install JetBrainsMono Nerd Font"
      },
      {
        "command": "jetvibe.useNerdFontInTerminal",
        "title": "JetVibe: Use Nerd Font in Terminal"
      },
      {
        "command": "jetvibe.setupP10kWSL",
        "title": "JetVibe: Setup Powerlevel10k on WSL"
      },
      {
        "command": "jetvibe.openLocalHistory",
        "title": "JetVibe: Open Local History"
      },
      {
        "command": "jetvibe.showLocalHistoryStats",
        "title": "JetVibe: Show Local History Stats"
      },
      {
        "command": "jetvibe.cleanupLocalHistory",
        "title": "JetVibe: Cleanup Local History"
      }
    ],
    "themes": [
      {
        "label": "JetVibe Darcula",
        "uiTheme": "vs-dark",
        "path": "./themes/jetvibe-darcula.json"
      }
    ],
    "configurationDefaults": {
      "editor.formatOnSave": true,
      "window.titleBarStyle": "custom",
      "window.zoomLevel": -1.25,
      "workbench.tree.indent": 8,
      "workbench.editor.tabSizing": "shrink",
      "workbench.colorTheme": "JetVibe Darcula",
      "editor.fontFamily": "'JetBrains Mono', Consolas, monospace",
      "editor.fontWeight": "400",
      "editor.fontLigatures": true,
      "editor.fontSize": 12,
      "editor.lineHeight": 20,
      "editor.letterSpacing": -0.1,
      "terminal.integrated.fontFamily": "JetBrainsMono Nerd Font Mono",
      "terminal.integrated.fontSize": 13,
      "terminal.integrated.lineHeight": 1.1,
      "terminal.integrated.letterSpacing": 0,
      "workbench.list.smoothScrolling": true,
      "editor.minimap.enabled": false,
      "breadcrumbs.enabled": true,
      "editor.bracketPairColorization.enabled": false,
      "editor.lineNumbers": "on",
      "workbench.statusBar.visible": true
    },
    "keybindings": [
      {
        "command": "workbench.action.showCommands",
        "key": "ctrl+shift+a",
        "when": "editorTextFocus"
      },
      {
        "command": "editor.action.formatDocument",
        "key": "ctrl+alt+l",
        "when": "editorHasDocumentFormattingProvider && editorTextFocus && !editorReadonly"
      },
      {
        "command": "editor.action.organizeImports",
        "key": "ctrl+alt+o",
        "when": "editorHasOrganizeImportsProvider && editorTextFocus && !editorReadonly"
      },
      {
        "command": "editor.action.rename",
        "key": "shift+f6",
        "when": "editorHasRenameProvider && editorTextFocus && !editorReadonly"
      },
      {
        "command": "workbench.action.findInFiles",
        "key": "ctrl+shift+f"
      },
      {
        "command": "workbench.action.replaceInFiles",
        "key": "ctrl+shift+r"
      },
      {
        "command": "editor.action.commentLine",
        "key": "ctrl+/",
        "when": "editorTextFocus && !editorReadonly"
      },
      {
        "command": "editor.action.blockComment",
        "key": "ctrl+shift+/",
        "when": "editorTextFocus && !editorReadonly"
      },
      {
        "command": "editor.action.copyLinesDownAction",
        "key": "ctrl+d",
        "when": "editorTextFocus && !editorReadonly"
      },
      {
        "command": "editor.action.deleteLines",
        "key": "ctrl+y",
        "when": "editorTextFocus && !editorReadonly"
      },
      {
        "command": "editor.action.joinLines",
        "key": "ctrl+shift+j",
        "when": "editorTextFocus && !editorReadonly"
      },
      {
        "command": "editor.action.moveLinesDownAction",
        "key": "shift+alt+down",
        "when": "editorTextFocus && !editorReadonly"
      },
      {
        "command": "editor.action.moveLinesUpAction",
        "key": "shift+alt+up",
        "when": "editorTextFocus && !editorReadonly"
      },
      {
        "command": "editor.action.quickFix",
        "key": "alt+enter",
        "when": "editorHasCodeActionsProvider && editorTextFocus && !editorReadonly"
      },
      {
        "command": "editor.action.revealDefinition",
        "key": "ctrl+b",
        "when": "editorHasDefinitionProvider && editorTextFocus"
      },
      {
        "command": "editor.action.peekDefinition",
        "key": "ctrl+shift+i",
        "when": "editorHasDefinitionProvider && editorTextFocus"
      },
      {
        "command": "editor.action.goToImplementation",
        "key": "ctrl+alt+b",
        "when": "editorHasImplementationProvider && editorTextFocus"
      },
      {
        "command": "editor.action.goToTypeDefinition",
        "key": "ctrl+shift+b",
        "when": "editorHasTypeDefinitionProvider && editorTextFocus"
      },
      {
        "command": "editor.action.referenceSearch.trigger",
        "key": "alt+f7",
        "when": "editorHasReferenceProvider && editorTextFocus"
      },
      {
        "command": "workbench.action.quickOpenPreviousRecentlyUsedEditorInGroup",
        "key": "ctrl+e"
      },
      {
        "command": "workbench.action.quickOpen",
        "key": "ctrl+shift+n"
      },
      {
        "command": "workbench.action.navigateBack",
        "key": "ctrl+alt+left"
      },
      {
        "command": "workbench.action.navigateForward",
        "key": "ctrl+alt+right"
      },
      {
        "command": "workbench.action.terminal.toggleTerminal",
        "key": "alt+f12"
      },
      {
        "command": "editor.action.addSelectionToNextFindMatch",
        "key": "alt+j",
        "when": "editorFocus"
      },
      {
        "command": "editor.action.removeSelectionFromNextFindMatch",
        "key": "shift+alt+j",
        "when": "editorFocus && editorHasMultipleSelections"
      },
      {
        "command": "editor.action.smartSelect.grow",
        "key": "ctrl+w",
        "when": "editorTextFocus"
      },
      {
        "command": "editor.action.smartSelect.shrink",
        "key": "ctrl+shift+w",
        "when": "editorTextFocus"
      },
      {
        "command": "workbench.action.debug.run",
        "key": "shift+f10"
      },
      {
        "command": "workbench.action.debug.start",
        "key": "shift+f9"
      }
    ]
  },
  "files": [
    "themes/**",
    "fonts/**",
    "images/**",
    "extension.js",
    "localHistory.js",
    "README.md",
    "LICENSE*"
  ],
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/igorhaf/jetvibe.git"
  },
  "homepage": "https://github.com/igorhaf/jetvibe#readme",
  "bugs": {
    "url": "https://github.com/igorhaf/jetvibe/issues"
  },
  "extensionPack": [
    "PKief.material-icon-theme",
    "mikestead.dotenv",
    "eamodio.gitlens",
    "cweijan.vscode-database-client2",
    "Vue.volar"
  ],
  "extensionDependencies": [],
  "main": "./extension.js"
}