{
  "name": "jetvibe",
  "displayName": "JetVibe",
  "description": "Sets up VSCode to Jetbrains style and includes ready-to-use extras.",
  "version": "0.0.42",
  "publisher": "igorhaf",
  "engines": {
    "vscode": "^1.84.0"
  },
  "extensionKind": [
    "ui"
  ],
  "icon": "images/icon.png",
  "categories": [
    "Themes",
    "Keymaps",
    "Programming Languages"
  ],
  "keywords": [
    "phpstorm",
    "darcula",
    "jetbrains",
    "keymap",
    "theme",
    "icons"
  ],
  "contributes": {
    "commands": [
      {
        "command": "jetvibe.installFont",
        "title": "JetVibe: Install JetBrains Mono"
      },
      {
        "command": "jetvibe.installNerdFont",
        "title": "JetVibe: Install JetBrainsMono Nerd Font"
      },
      {
        "command": "jetvibe.useNerdFontInTerminal",
        "title": "JetVibe: Use Nerd Font in Terminal"
      },
      {
        "command": "jetvibe.setupP10kWSL",
        "title": "JetVibe: Setup Powerlevel10k on WSL"
      },
      {
        "command": "jetvibe.openLocalHistory",
        "title": "JetVibe: Open Local History"
      },
      {
        "command": "jetvibe.showLocalHistoryStats",
        "title": "JetVibe: Show Local History Stats"
      },
      {
        "command": "jetvibe.cleanupLocalHistory",
        "title": "JetVibe: Cleanup Local History"
      },
      {
        "command": "jetvibe.diffWithLatestSnapshot",
        "title": "JetVibe: Diff With Latest Snapshot"
      },
      {
        "command": "jetvibe.openSnapshotDiff",
        "title": "JetVibe: Diff Snapshot With Previous"
      },
      {
        "command": "jetvibe.diffFolderWithLocal",
        "title": "JetVibe: Diff Folder With Local Directory"
      },
      {
        "command": "jetvibe.switchToNextRevision",
        "title": "JetVibe: Switch To Diff With Next Revision"
      }
    ],
    "themes": [
      {
        "label": "JetVibe Darcula",
        "uiTheme": "vs-dark",
        "path": "./themes/jetvibe-darcula.json"
      }
    ],
    "views": {
      "explorer": [
        {
          "id": "jetvibeLocalHistory",
          "name": "JetVibe · Local History"
        }
      ]
    },
    "configurationDefaults": {
      "editor.formatOnSave": true,
      "window.titleBarStyle": "custom",
      "window.zoomLevel": -1.25,
      "workbench.tree.indent": 8,
      "workbench.editor.tabSizing": "shrink",
      "workbench.colorTheme": "JetVibe Darcula",
      "editor.fontFamily": "'JetBrains Mono', Consolas, monospace",
      "editor.fontWeight": "400",
      "editor.fontLigatures": true,
      "editor.fontSize": 12,
      "editor.lineHeight": 20,
      "editor.letterSpacing": -0.1,
      "terminal.integrated.fontFamily": "JetBrainsMono Nerd Font Mono",
      "terminal.integrated.fontSize": 13,
      "terminal.integrated.lineHeight": 1.1,
      "terminal.integrated.letterSpacing": 0,
      "workbench.list.smoothScrolling": true,
      "editor.minimap.enabled": false,
      "breadcrumbs.enabled": true,
      "editor.bracketPairColorization.enabled": false,
      "editor.lineNumbers": "on",
      "workbench.statusBar.visible": true,
      "php.validate.enable": false,
      "intelephense.environment.phpVersion": "8.2",
      "intelephense.files.associations": [
        "*.php",
        "*.phtml"
      ],
      "files.associations": {
        "*.blade.php": "blade",
        "*.jsx": "javascriptreact",
        "*.tsx": "typescriptreact",
        "*.ejs": "ejs",
        "*.hbs": "handlebars",
        "*.graphql": "graphql",
        "*.gql": "graphql",
        "*.proto": "proto3",
        "*.prisma": "prisma",
        "*.tf": "terraform",
        "*.tfvars": "terraform",
        "*.cue": "cue",
        "*.jsonnet": "jsonnet",
        "*.libsonnet": "jsonnet",
        "*.jinja": "jinja-html",
        "*.j2": "jinja-html",
        "*.twig": "twig",
        "*.toml": "toml",
        "*.ini": "ini",
        "*.csv": "csv",
        "*.tsv": "tsv",
        "nginx.conf": "nginx"
      },
      "tailwindCSS.includeLanguages": {
        "blade": "html",
        "vue": "html",
        "php": "html",
        "ejs": "html",
        "jinja-html": "html",
        "twig": "html",
        "svelte": "html",
        "astro": "html"
      },
      "typescript.tsdk": "node_modules/typescript/lib",
      "editor.quickSuggestions": {
        "strings": true
      },
      "markdown.mermaid.enabled": true
    },
    "configuration": {
      "title": "JetVibe · Local History",
      "properties": {
        "jetvibe.localHistory.enabled": {
          "type": "boolean",
          "default": true,
          "description": "Ativa/Desativa o Histórico Local (criação automática de snapshots ao abrir/salvar arquivos)."
        },
        "jetvibe.localHistory.maxSnapshotsPerFile": {
          "type": "number",
          "default": 50,
          "minimum": 1,
          "maximum": 500,
          "description": "Número máximo de snapshots mantidos por arquivo."
        },
        "jetvibe.localHistory.includeGlobs": {
          "type": "array",
          "default": [
            "**/*"
          ],
          "items": {
            "type": "string"
          },
          "description": "Padrões (glob) a incluir para criação de snapshots. Use '**/*' para todos."
        },
        "jetvibe.localHistory.excludeGlobs": {
          "type": "array",
          "default": [
            "**/.git/**",
            "**/node_modules/**",
            "**/*.log"
          ],
          "items": {
            "type": "string"
          },
          "description": "Padrões (glob) a excluir da criação de snapshots. Tem precedência sobre include."
        }
      }
    }
  },
  "files": [
    "themes/**",
    "fonts/**",
    "images/**",
    "extension.js",
    "localHistory.js",
    "README.md",
    "LICENSE*"
  ],
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/igorhaf/jetvibe.git"
  },
  "homepage": "https://github.com/igorhaf/jetvibe#readme",
  "bugs": {
    "url": "https://github.com/igorhaf/jetvibe/issues"
  },
  "extensionPack": [
    "PKief.material-icon-theme",
    "mikestead.dotenv",
    "cweijan.vscode-database-client2",
    "Vue.volar",
    "bmewburn.vscode-intelephense-client",
    "bradlc.vscode-tailwindcss",
    "onecentlin.laravel-blade",
    "redhat.vscode-yaml",
    "chr4.nginx",
    "Angular.ng-template",
    "styled-components.vscode-styled-components",
    "clinyong.vscode-css-modules",
    "ms-azuretools.vscode-docker",
    "redhat.vscode-xml",
    "GraphQL.vscode-graphql",
    "GraphQL.vscode-graphql-syntax",
    "Prisma.prisma",
    "zxh404.vscode-proto3",
    "42Crunch.vscode-openapi",
    "HashiCorp.terraform",
    "ms-kubernetes-tools.vscode-kubernetes-tools",
    "Tim-Koehler.helm-intellisense",
    "tamasfe.even-better-toml",
    "DavidWang.ini-for-vscode",
    "mechatroner.rainbow-csv",
    "jnoortheen.nix-ide",
    "golang.Go",
    "rust-lang.rust-analyzer",
    "ms-python.python",
    "ms-python.vscode-pylance",
    "bierner.markdown-mermaid",
    "jebbs.plantuml",
    "svelte.svelte-vscode",
    "astro-build.astro-vscode",
    "csstools.postcss",
    "digitalbrainstem.javascript-ejs-support",
    "samuelcolvin.jinjahtml",
    "whatwedo.twig"
  ],
  "extensionDependencies": [],
  "main": "./extension.js"
}